<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.adm.pantallas.Principal"
    android:background="#37474F">


    <Button
        android:id="@+id/btnSalida"
        android:layout_width="273dp"
        android:layout_height="78dp"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="378dp"
        android:background="@drawable/button"
        android:onClick="checkCodigo"
        android:text="@string/Salida_boton"
        android:textColor="#212121"
        android:textColorHint="#9E9E9E"
        android:textSize="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.505" /><![CDATA[/>


    ]]>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/LayoutOpciones"
        android:layout_width="0dp"
        android:layout_height="391dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:visibility="visible"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ScrollView
            android:layout_width="50dp"
            android:layout_height="1280dp"
            tools:layout_editor_absoluteX="320dp"
            tools:layout_editor_absoluteY="-24dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" />
        </ScrollView>

        <EditText
            android:id="@+id/etDistancia"
            android:layout_width="73dp"
            android:layout_height="30dp"
            android:layout_marginEnd="5dp"
            android:backgroundTint="@color/colorAccent"
            android:ems="10"
            android:textColorHint="#9E9E9E"
            android:textColor="#9E9E9E"
            android:inputType="numberDecimal"
            tools:ignore="LabelFor,RtlHardcoded"
            app:layout_constraintTop_toTopOf="@+id/txtDistancia"
            android:layout_marginTop="30dp"
            android:layout_marginRight="5dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.641"
            android:layout_marginStart="8dp"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/txtDistancia"
            android:layout_width="173dp"
            android:layout_height="60dp"
            android:text="@string/Salida_Velocidad"
            android:textAlignment="textStart"
            android:textColor="#9E9E9E"
            android:textSize="22sp"
            tools:ignore="RtlHardcoded"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="38dp"
            app:layout_constraintTop_toBottomOf="@+id/textView8"
            android:layout_marginStart="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.044"
            android:layout_marginEnd="8dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="165dp"
            android:layout_height="55dp"
            android:text="@string/Salida_Distancia"
            android:textAlignment="textStart"
            android:textColor="#9E9E9E"
            android:textSize="22sp"
            tools:ignore="RtlHardcoded"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="27dp"
            app:layout_constraintTop_toBottomOf="@+id/txtTiempo"
            android:layout_marginStart="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.042"
            android:layout_marginEnd="8dp" />

        <EditText
            android:id="@+id/editText"
            android:layout_width="72dp"
            android:layout_height="48dp"
            android:layout_marginEnd="5dp"
            android:backgroundTint="@color/colorAccent"
            android:ems="10"
            android:textColorHint="#9E9E9E"
            android:textColor="#9E9E9E"
            android:inputType="numberDecimal"
            tools:ignore="LabelFor,RtlHardcoded"
            app:layout_constraintTop_toTopOf="@+id/textView8"
            android:layout_marginRight="5dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.646"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            android:textSize="22sp"
            app:layout_constraintVertical_bias="0.0"
            android:layout_marginStart="8dp" />

        <EditText
            android:id="@+id/etTiempo"
            android:layout_width="70dp"
            android:layout_height="45dp"
            android:layout_marginEnd="5dp"
            android:backgroundTint="@color/colorAccent"
            android:ems="10"
            android:textSize="22sp"
            android:textColorHint="#9E9E9E"
            android:textColor="#9E9E9E"
            android:inputType="numberDecimal"
            tools:ignore="LabelFor,RtlHardcoded"
            app:layout_constraintTop_toTopOf="@+id/txtTiempo"
            android:layout_marginRight="5dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.648"
            android:layout_marginStart="8dp" />

        <TextView
            android:id="@+id/txtTiempo"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/colorAccent"
            android:text="@string/Salida_Tiempo"
            android:textAlignment="textStart"
            android:textColorHint="#9E9E9E"
            android:textColor="#9E9E9E"
            android:textSize="22sp"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="RtlHardcoded"
            android:layout_marginTop="73dp"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginStart="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.043"
            android:layout_marginEnd="8dp" />
    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        app:layout_constraintGuide_begin="20dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="20dp"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="km"
        android:textColor="#9E9E9E"
        android:textSize="22sp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/LayoutOpciones"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintHorizontal_bias="0.809"
        app:layout_constraintVertical_bias="0.312"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="km/s"
        android:textColor="#9E9E9E"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.852"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="minutos"
        android:textColor="#9E9E9E"
        android:textSize="22sp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="91dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.927"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

</android.support.constraint.ConstraintLayout>
